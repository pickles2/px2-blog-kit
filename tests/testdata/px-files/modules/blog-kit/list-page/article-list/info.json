{
    "name": "記事リスト - Article list",
    "interface": {
        "fields": {
            "blog_id": {
                "fieldType": "input",
                "type": "text",
                "label": "ブログID",
                "rows": 1
            },
            "description": {
                "fieldType": "input",
                "type": "text",
                "label": "ブログの説明",
                "rows": 1
            },
            "dpp": {
                "fieldType": "input",
                "type": "select",
                "label": "ページあたりの記事件数",
                "options": [
                    {
                        "value": "5",
                        "label": "5"
                    },
                    {
                        "value": "10",
                        "label": "10"
                    },
                    {
                        "value": "15",
                        "label": "15"
                    },
                    {
                        "value": "20",
                        "label": "20"
                    },
                    {
                        "value": "30",
                        "label": "30"
                    },
                    {
                        "value": "40",
                        "label": "40"
                    },
                    {
                        "value": "50",
                        "label": "50"
                    },
                    {
                        "value": "100",
                        "label": "100"
                    }
                ],
                "default": "10"
            },
            "orderby": {
                "fieldType": "input",
                "type": "text",
                "label": "並び順のキー",
                "rows": 1,
                "default": "update_date"
            },
            "scending": {
                "fieldType": "input",
                "type": "select",
                "label": "昇順/降順",
                "options": [
                    {
                        "value": "asc",
                        "label": "昇順"
                    },
                    {
                        "value": "desc",
                        "label": "降順"
                    }
                ],
                "default": "desc"
            },
            "path_feeds": {
                "fieldType": "input",
                "type": "text",
                "label": "RSSフィードの出力先パス",
                "rows": 1
            }
        }
    }
}